{
  "name": "unitygridplaza",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "cd client && vite",
    "dev:server": "cd server && nodemon --require dotenv/config server.js",
    "build": "cd client && vite build",
    "start": "node server/server.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
